{
  "realm": "myrealm",
  "clients": [
    {
      "clientId": "myclient",
      "publicClient": true,
      "directAccessGrantsEnabled": true,
      "secret": "myclient-secret"
    }
  ],
  "users": [
    {
      "username": "myuser",
      "enabled": true,
      "credentials": [
        {
          "type": "password",
          "value": "mypassword"
        }
      ]
    }
  ],
  "roles": [
    {
      "name": "myrole"
    }
  ],
  "rolesDefaultClient": ["myrole"],
  "authenticationFlowBindingOverrides": {
    "browser": "mybrowserflow"
  },
  "browserFlows": ["mybrowserflow"],
  "clientAuthenticationFlow": "myclientauthenticationflow"
}
